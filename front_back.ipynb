    "def front_back(s):\n",
    "    \n",
    "    f = s[-1]\n",
    "    l = s[0]\n",
    "    \n",
    "    if len(s)<= 1:\n",
    "        return s\n",
    "    else:\n",
    "        return f + s[1:-1] + l"
